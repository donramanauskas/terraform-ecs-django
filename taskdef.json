{
  "family": "django-app",
  "containerDefinitions": [
    {
      "name": "django-app",
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "cpu": 10,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 8000
        }
      ],
      "entryPoint": null,
      "command": [
        "gunicorn",
        "-w",
        "3",
        "-b",
        ":8000",
        "hello_django.wsgi:application"
      ],
      "secrets": [
        {
          "name": "RDS_DB_NAME"
        },
        {
          "name": "RDS_HOSTNAME"
        },
        {
          "name": "RDS_PASSWORD"
        },
        {
          "name": "RDS_PORT"
        },
        {
          "name": "RDS_USERNAME",
          "value": "bbduser"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": null,
        "options": {
          "awslogs-group": "/ecs/django-app",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "django-app-log-stream"
        }
      }
    }
  ]
}